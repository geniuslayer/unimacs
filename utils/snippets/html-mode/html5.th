#name : html5 thymeleaf
#group : meta
# --
<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">
    <head>
        <meta charset="utf-8"/>
        <title>${1:Home}</title>
    </head>
    <body>
        <h2 th:text="#{app.title}">App Title</h2>
        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                </tr>
            </thead>
            <tbody>
                <tr th:each="${2:user} : $\{${3:users}\}">
                    <td th:text="$\{$2.id\}">Id</td>
                    <td th:text="$\{$2.name\}">Name</td>
                </tr>
            </tbody>
        </table>
    </body>
</html>
